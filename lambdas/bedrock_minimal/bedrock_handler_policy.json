{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "BedrockAgentInvocation",
      "Effect": "Allow",
      "Action": ["bedrock:InvokeAgent"],
      "Resource": [
        "arn:aws:bedrock:us-east-1:354468042457:agent-alias/WoQ1uB3z5m/DEFAULT",
        "arn:aws:bedrock:us-east-1:354468042457:agent/WoQ1uB3z5m"
      ]
    },
    {
      "Sid": "CloudWatchLogsAccess",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:us-east-1:354468042457:log-group:/aws/lambda/policy-mate-bedrock-handler:*"
    }
  ]
}
